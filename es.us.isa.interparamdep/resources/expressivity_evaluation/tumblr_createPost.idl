IF title THEN type='text'|'link'|'chat';

IF caption THEN type='text'|'link'|'chat';

IF data THEN type='text'|'link'|'chat';

IF source THEN type='photo'|'quote';

IF body THEN type='text';

IF link THEN type='photo';

IF data64 THEN type='photo';

IF quote THEN type='quote';

IF url THEN type='link';

IF description THEN type='link';

IF thumbnail THEN type='link';

IF excerpt THEN type='link';

IF author THEN type='link';

IF conversation THEN type='chat';

IF external_url THEN type='audio';

IF embed THEN type='video';

IF type='photo' THEN OnlyOne(source, data, data64);

IF type='audio' THEN OnlyOne(external_url, data);

IF type='video' THEN OnlyOne(embed, data);
